{
  "name": "geoviz",
  "version": "0.0.1",
  "description": "d3-based javascript library for thematic maps",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/index.min.js",
  "unpkg": "dist/index.min.js",
  "exports": {
    "umd": "./dist/index.min.js",
    "default": "./src/index.js"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rollup --config",
    "prepare": "npm run build",
    "jsdoc": "jsdoc -a all -c jsdoc.json -r -R README.md index.js -d docs/jsdoc && jsdoc2md --files index.js > docs/API.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neocarto/geoviz.git"
  },
  "keywords": [
    "maps",
    "cartography",
    "d3"
  ],
  "author": "Nicolas Lambert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neocarto/geoviz/issues"
  },
  "homepage": "https://github.com/neocarto/geoviz#readme",
  "dependencies": {
    "d3-geo": "^3.1.0",
    "d3-geo-projection": "^4.0.0",
    "d3-selection": "^3.0.0",
    "dicopal": "^0.4.0",
    "statsbreaks": "^1.0.3"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "docdash": "^2.0.1",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0"
  }
}
